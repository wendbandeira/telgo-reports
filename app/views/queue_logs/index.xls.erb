<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Data</Data></Cell>
        <Cell><Data ss:Type="String">Tempo Fila</Data></Cell>
        <Cell><Data ss:Type="String">Tempo Agente</Data></Cell>
        <Cell><Data ss:Type="String">Faixa de Hor√°rio</Data></Cell>
        <Cell><Data ss:Type="String">Fila Chamada</Data></Cell>
        <Cell><Data ss:Type="String">Agente</Data></Cell>
        <Cell><Data ss:Type="String">Evento</Data></Cell>
        <Cell><Data ss:Type="String">Origem</Data></Cell>
      </Row>
    <% @queue_log.each do |queue| %>
      <Row>
        <Cell><Data ss:Type="String"><%= queue.time.to_datetime.strftime("%d-%m-%Y - %H:%M" ) %></Data></Cell>
        <Cell><Data ss:Type="String"><%= Time.at(queue.tempofila.to_i).utc.strftime("%H:%M:%S") %></Data></Cell>
        <Cell><Data ss:Type="String"><%= Time.at(queue.tempoagente.to_i).utc.strftime("%H:%M:%S") %></Data></Cell>
        <Cell><Data ss:Type="String"><%= CodigosHelper.range_hour(queue.time.to_datetime.strftime("%H")) %></Data></Cell>
        <Cell><Data ss:Type="String"><%= queue.queuename %></Data></Cell>
        <Cell><Data ss:Type="String"><%= queue.agent %></Data></Cell>
        <Cell><Data ss:Type="String"><%= queue.status %></Data></Cell>
        <Cell><Data ss:Type="String"><%= queue.origem %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
